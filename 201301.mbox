    private BundleContext bundleContext=3B
    private BundleAccessor bundleAccessor=3B
    private boolean bundlesChanged =3D false=3B
    private ServletContext servletContext=3B

    public void init(Configuration configuration) throws ConfigurationExcep=
tion {
        osgiHost =3D (OsgiHost) servletContext.getAttribute(StrutsOsgiListe=
ner.OSGI_HOST)=3B
        bundleContext =3D osgiHost.getBundleContext()=3B
        bundleAccessor.setBundleContext(bundleContext)=3B
        bundleAccessor.setOsgiHost(osgiHost)=3B
        this.configuration =3D configuration=3B

        //this class loader interface can be used by other plugins to looku=
p resources
        //from the bundles. A temporary class loader interface is set durin=
g other configuration
