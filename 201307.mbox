<configuration>
	<includedArtifacts>
		<includedArtifact>
			<groupId>com.somegroup</groupId>
			<artifactId>someartifact</artifactId>
			<version>1.0.1</version>
		</includedArtifact>
	</includedArtifacts>
	........

